#include <physical_layouts.dtsi>

/ {
    charybdis_physical_layout: charybdis_physical_layout {
        compatible = "zmk,physical-layout";
        display-name = "Charybdis";
        transform = <&default_transform>;
        kscan = <&kscan0>;
        keys
        // Left Half - Main Keys
        //                     w    h    x    y    rot  rx   ry
        = <&key_physical_attrs 100  100  0    0    0    0    0>    // L00
        , <&key_physical_attrs 100  100  100  0    0    0    0>    // L01
        , <&key_physical_attrs 100  100  200  0    0    0    0>    // L02
        , <&key_physical_attrs 100  100  300  0    0    0    0>    // L03
        , <&key_physical_attrs 100  100  400  0    0    0    0>    // L04
        , <&key_physical_attrs 100  100  500  0    0    0    0>    // L05
        , <&key_physical_attrs 100  100  0    100  0    0    0>    // L10
        , <&key_physical_attrs 100  100  100  100  0    0    0>    // L11
        , <&key_physical_attrs 100  100  200  100  0    0    0>    // L12
        , <&key_physical_attrs 100  100  300  100  0    0    0>    // L13
        , <&key_physical_attrs 100  100  400  100  0    0    0>    // L14
        , <&key_physical_attrs 100  100  500  100  0    0    0>    // L15
        , <&key_physical_attrs 100  100  0    200  0    0    0>    // L20
        , <&key_physical_attrs 100  100  100  200  0    0    0>    // L21
        , <&key_physical_attrs 100  100  200  200  0    0    0>    // L22
        , <&key_physical_attrs 100  100  300  200  0    0    0>    // L23
        , <&key_physical_attrs 100  100  400  200  0    0    0>    // L24
        , <&key_physical_attrs 100  100  500  200  0    0    0>    // L25

        // Right Half - Main Keys
        , <&key_physical_attrs 100  100  700  0    0    0    0>    // R00
        , <&key_physical_attrs 100  100  800  0    0    0    0>    // R01
        , <&key_physical_attrs 100  100  900  0    0    0    0>    // R02
        , <&key_physical_attrs 100  100  1000 0    0    0    0>    // R03
        , <&key_physical_attrs 100  100  1100 0    0    0    0>    // R04
        , <&key_physical_attrs 100  100  1200 0    0    0    0>    // R05
        , <&key_physical_attrs 100  100  700  100  0    0    0>    // R10
        , <&key_physical_attrs 100  100  800  100  0    0    0>    // R11
        , <&key_physical_attrs 100  100  900  100  0    0    0>    // R12
        , <&key_physical_attrs 100  100  1000 100  0    0    0>    // R13
        , <&key_physical_attrs 100  100  1100 100  0    0    0>    // R14
        , <&key_physical_attrs 100  100  1200 100  0    0    0>    // R15
        , <&key_physical_attrs 100  100  700  200  0    0    0>    // R20
        , <&key_physical_attrs 100  100  800  200  0    0    0>    // R21
        , <&key_physical_attrs 100  100  900  200  0    0    0>    // R22
        , <&key_physical_attrs 100  100  1000 200  0    0    0>    // R23
        , <&key_physical_attrs 100  100  1100 200  0    0    0>    // R24
        , <&key_physical_attrs 100  100  1200 200  0    0    0>    // R25

        // Thumb Clusters (3 keys each side)
        , <&key_physical_attrs 100  100  100  300  15   0    0>    // LT0
        , <&key_physical_attrs 100  100  200  300  15   0    0>    // LT1
        , <&key_physical_attrs 100  100  300  300  15   0    0>    // LT2
        , <&key_physical_attrs 100  100  900  300  -15  0    0>    // RT0
        , <&key_physical_attrs 100  100  1000 300  -15  0    0>    // RT1
        , <&key_physical_attrs 100  100  1100 300  -15  0    0>    // RT2
        ;
    };
};